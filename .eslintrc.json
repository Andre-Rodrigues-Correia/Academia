{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "airbnb-base",
    "overrides": [
        
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
       "linebreak-style": "off",
       "import/extensions": ["error", "ignorePackages"],
       "import/no-unresolved": "off",
       "no-useless-catch": "off",
       "max-len": "off"
    }
}
